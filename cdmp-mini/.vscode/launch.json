{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Fast Debug - Test Only",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/test/iam-apiserver/user/change_passwd",
            "args": [
                "-test.v",
                "-test.run",
                "TestChangePassword_Functional"
            ],
            "console": "integratedTerminal",
            "showLog": false,
            "trace": "off",
            "logOutput": false,
            "env": {
                "GODEBUG": "asyncpreemptoff=1"
            }
        },
        {
            "name": "Attach to Running Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        },
        {
            "name": "Attach to IAM Server",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processName": "apiserver"
        }
    ]
}