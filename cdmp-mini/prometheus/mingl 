sudo systemctl status grafana-server


 curl http://127.0.0.1:8088/metrics | grep -E 'user_cache_hits_total{type='

curl http://127.0.0.1:8088/metrics | grep -E 'user_cache_errors_total'


sum(rate(user_cache_hits_total{type="hit"}[5m])) /
 (sum(rate(user_cache_hits_total{type="hit"}[5m])) + sum(rate(user_cache_hits_total{type="miss"}[5m]))) * 100